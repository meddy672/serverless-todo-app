{
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "spec_dir": "spec",
  "spec_files": [
    "**/*[sS]pec.ts"
  ],
  "helpers": [
    "helpers/helper.js",
    "../../node_modules/@babel/register/lib/node.js"
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
